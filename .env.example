# .env
# This file contains secret keys and is ignored by git.
# To connect to your Supabase project, copy these values from:
# Supabase Dashboard -> Project Settings -> API

# The URL of your Supabase project (e.g., https://xyz.supabase.co)
SUPABASE_URL=

# The Anon/Public key for safe frontend reading and Edge Function querying
SUPABASE_ANON_KEY=

# The Service Role key (DANGEROUS: Bypass RLS). Used ONLY in secure GitHub Actions for data ingestion.
SUPABASE_SERVICE_ROLE_KEY=

# Optional helper for CLI linking in npm scripts: npm run db:link
SUPABASE_PROJECT_REF=

# Optional direct DB connection URL for npm run db:push:url
SUPABASE_DB_URL=
